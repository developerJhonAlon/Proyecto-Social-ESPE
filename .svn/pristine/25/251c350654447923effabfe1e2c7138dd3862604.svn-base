                                                             rendered="#{reportesController.cobertura ne 1}"
                                                             converter="omnifaces.SelectItemsConverter"
                                                             value="#{reportesController.provincia}"
                                                             effect="fade"
                                                             filter="false">
                                                <f:selectItem itemLabel="[-TODOS-]"/>
                                                <f:selectItems value="#{reportesController.provincias}" var="provinciaProy" 
                                                               itemValue="#{provinciaProy}" itemLabel="#{provinciaProy.proNombre}"
                                                               >
                                                </f:selectItems>
                                                <p:ajax event="change" update=":formPrincipal:somCantonesProy, :formPrincipal:somParroquiasProy" listener="#{reportesController.seleccionarProvincia}"/>
                                            </p:selectOneMenu>

                                            <p:outputLabel value="Cantón:" for="somCantonesProy" rendered="#{reportesController.cobertura eq 0 or reportesController.cobertura gt 2}"/>
