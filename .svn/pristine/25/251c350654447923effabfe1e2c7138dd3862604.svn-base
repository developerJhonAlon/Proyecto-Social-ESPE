                                                              rendered="#{reportesController.cobertura eq 0 or reportesController.cobertura gt 2}"
                                                              value="#{reportesController.canton}"
                                                              converter="omnifaces.SelectItemsConverter"
                                                              effect="fade"
                                                              filter="false"
                                                              >
                                                <f:selectItem itemLabel="[-TODOS-]"/>
                                                <f:selectItems value="#{reportesController.provincia.seaCantonList}" var="cantonProy" 
                                                               itemValue="#{cantonProy}" itemLabel="#{cantonProy.canNombre}"
                                                               >
                                                </f:selectItems>
                                                <p:ajax event="change" update=":formPrincipal:somParroquiasProy"/>
                                            </p:selectOneMenu> 

                                            <p:outputLabel value="Parroquias:" for="somParroquiasProy" rendered="#{reportesController.cobertura eq 0 or reportesController.cobertura gt 3}"/>
                                            <p:selectOneMenu  id="somParroquiasProy"
                                                              rendered="#{reportesController.cobertura eq 0 or reportesController.cobertura gt 3}"
                                                              value="#{reportesController.parroquia}"
                                                              converter="omnifaces.SelectItemsConverter"
                                                              effect="fade"
                                                              filter="false">
                                                <f:selectItem itemLabel="[-TODOS-]"/>
                                                <f:selectItems value="#{reportesController.canton.seaParroquiaList}" var="parroquiaProy" 
                                                               itemValue="#{parroquiaProy}" itemLabel="#{parroquiaProy.prqNombre}"
                                                               >
                                                </f:selectItems>
                                            </p:selectOneMenu>                                             

                                            <p:outputLabel value="Sede:"/>
                                            <p:selectOneMenu value="#{reportesController.codigoSede}"
                                                             effect="fade"
                                                             filter="false">
                                                <f:selectItem itemLabel="[-TODOS-]" itemValue="" />
                                                <f:selectItems value="#{reportesController.sedes}" var="sedeProy" 
                                                               itemValue="#{sedeProy.stvcampCode}" itemLabel="#{sedeProy.stvcampDesc}"
                                                               >
                                                </f:selectItems>
                                            </p:selectOneMenu>

                                            <p:outputLabel value="Docente:" rendered="#{reportesController.codigoReporte==3}"/>
                                            <p:selectOneMenu value="#{reportesController.usuario}"
                                                             rendered="#{reportesController.codigoReporte==3}"
                                                             converter="omnifaces.SelectItemsConverter"
                                                             effect="fade"
                                                             filter="true"
                                                             filterMatchMode="contains">
                                                <f:selectItem itemLabel="[-TODOS-]"/>
                                                <f:selectItems value="#{reportesController.docenteList}" var="docenteParti" 
                                                               itemValue="#{docenteParti}" itemLabel="#{docenteParti.usuCc.concat('-').concat(docenteParti.usuNombres).concat(' ').concat(docenteParti.usuApellidos)}"
                                                               >
                                                </f:selectItems>
                                            </p:selectOneMenu>
                                            <p:outputLabel value="Estudiante:" rendered="#{reportesController.codigoReporte==4}"/>
                                            <p:selectOneMenu value="#{reportesController.usuario}"
                                                             rendered="#{reportesController.codigoReporte==4}"
                                                             converter="omnifaces.SelectItemsConverter"
                                                             effect="fade"
                                                             filter="true"
                                                             filterMatchMode="contains">
                                                <f:selectItem itemLabel="[-TODOS-]"/>
                                                <f:selectItems value="#{reportesController.estudianteList}" var="docenteParti" 
                                                               itemValue="#{docenteParti}" itemLabel="#{docenteParti.usuCc.concat('-').concat(docenteParti.usuNombres).concat(' ').concat(docenteParti.usuApellidos)}"
                                                               >
                                                </f:selectItems>
                                            </p:selectOneMenu>

                                        </h:panelGrid> 

                                        <h:panelGrid columns="1" style="width: 100%; margin-bottom: 10px">
                                            <h:panelGrid columns="3">
                                                <h:commandLink rendered="true" target="_blank" actionListener="#{reportesController.generarReporteProyectos('PDF')}"
                                                               title="Exportar a PDF">
                                                    <p:graphicImage url="../../resources/images/pdf.png"/>
                                                </h:commandLink>
                                                <h:commandLink rendered="true" target="_blank" actionListener="#{reportesController.generarReporteProyectos('XLS')}"
                                                               title="Exportar a Excel">
                                                    <p:graphicImage url="../../resources/images/excel2.png" width="33"/>
                                                </h:commandLink>
                                                <h:commandLink rendered="true" target="_blank" actionListener="#{reportesController.generarReporteProyectos('WORD')}"
                                                               title="Exportar a Word">
                                                    <p:graphicImage url="../../resources/images/word.png" width="40"/>
                                                </h:commandLink>
                                            </h:panelGrid>
                                        </h:panelGrid>
                                    </p:fieldset>                                
                                </h:panelGrid>

                            </p:panel>

                        </p:panel>


                    </h:panelGrid>

                </p:panel>




