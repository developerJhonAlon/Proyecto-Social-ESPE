                                                               >
                                                </f:selectItems>
                                            </p:selectOneMenu>                                             

                                            <p:outputLabel value="Sede:" for="somSedes"/>
                                            <p:selectOneMenu  id="somSedes"
                                                              value="#{reportesController.codigoSede}"
                                                              effect="fade"
                                                              filter="true">
                                                <f:selectItem itemLabel="[-TODOS-]" itemValue="" />
                                                <f:selectItems value="#{reportesController.sedes}" var="sede" 
