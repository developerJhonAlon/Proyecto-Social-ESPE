                            <!--panelReportesProyectos-->
                            <p:panel id="panelReportesProyectos" rendered="#{reportesController.opcionMenuCodeSelected=='4'}" style="border: hidden; margin-left: 20px;">
                                <h:panelGrid cellspacing="5" columns="1" style="width: 100%">
                                    <h:panelGrid cellspacing="5" columns="2">
                                        <p:outputLabel value="Seleccione el Reporte:" for="somReportesProy"/>
                                        <p:selectOneMenu  id="somReportesProy"
                                                          value="#{reportesController.codigoReporte}">
                                            <f:selectItem itemLabel="Aprobación de Proyectos" itemValue="1" />
                                            <f:selectItem itemLabel="Proyectos Ejecutados" itemValue="2" />
                                            <f:selectItem itemLabel="Participación de Docentes" itemValue="3" />
                                            <f:selectItem itemLabel="Participación de Estudiantes" itemValue="4"/>
                                            <f:selectItem itemLabel="Beneficiarios" itemValue="5"/>
                                            <f:selectItem itemLabel="Presupuesto" itemValue="6"/>
                                            <f:selectItem itemLabel="Avance de Proyectos" itemValue="7"/>
                                            <f:selectItem itemLabel="Evaluación de Proyectos" itemValue="8"/>
                                            <f:selectItem itemLabel="Resultados de Proyectos" itemValue="9"/>
                                            <f:selectItem itemLabel="Certificados emitidos por Proyecto" itemValue="10"/>
                                            <p:ajax event="change" update=":formPrincipal:fsParametrosRepProy"/>
                                        </p:selectOneMenu>
                                    </h:panelGrid>


                                    <p:fieldset id="fsParametrosRepProy" legend="Parámetros del reporte:">
                                        <h:panelGrid columns="1">
                                            <p:outputLabel value="Fecha de ejecución"/>
