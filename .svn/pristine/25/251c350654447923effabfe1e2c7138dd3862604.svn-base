                                            <p:outputLabel value="Parroquia:" for="somParroquias"/>
                                            <p:selectOneMenu  id="somParroquias"
                                                              value="#{reportesController.parroquia}"
                                                              converter="omnifaces.SelectItemsConverter"
                                                              effect="fade"
                                                              filter="true">
                                                <f:selectItem itemLabel="[-TODOS-]"/>
                                                <f:selectItems value="#{reportesController.canton.seaParroquiaList}" var="parroquia" 
