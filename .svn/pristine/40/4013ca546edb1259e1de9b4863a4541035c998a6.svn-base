<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<ui:composition
    xmlns:ui="http://java.sun.com/jsf/facelets"
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:f="http://java.sun.com/jsf/core"
    xmlns:h="http://java.sun.com/jsf/html"
    template="/vista/plantilla.xhtml"
    xmlns:p="http://primefaces.org/ui"
    xmlns:c="http://java.sun.com/jsp/jstl/core"
    xmlns:pe="http://primefaces.org/ui/extensions"
    >

    <ui:param name="backingBean" value="#{programaVinculacionController}"/>
    <ui:param name="tituloPagina" value=""/>
    <ui:define name="editar"/> 
    <ui:define name="contenido">
        <script type="text/javascript" src="resources/js/programaVinculacion.js"/>
        <h:form id="formPrincipal1" enctype="multipart/form-data">
            <p:panel id="panelBase" header="Programas Vinculación" style="font-size: 11px; text-align: left; border: hidden; font-weight: bold">
                <p:growl id="message" showDetail="true" life="3000"></p:growl>            
                <h:panelGrid columns="3" id="hpCabecera" > 
                    <p:selectBooleanButton id="value2" value="#{programaVinculacionController.botonInicio}" onLabel="Agregar" offLabel="Programas Vinculación" onIcon="ui-icon-plus" offIcon="ui-icon-arrowreturnthick-1-w" style="width:100px">
                        <p:ajax update=":formPrincipal:formPrincipal1:panelBase" listener="#{programaVinculacionController.inicioProgramas}"/>
                    </p:selectBooleanButton>
                    <h:outputText  value ="#{programaVinculacionController.nombreProgSeleccionado}"/> 
                </h:panelGrid>
                <p:panel id="inicio" visible="#{programaVinculacionController.botonInicio}" style="font-size: 11px; text-align: left; border: hidden; font-weight: bold">
                    <p:dataTable id="dtProgramaVinculacion" value="#{programaVinculacionController.vinProgrList}" var="progr" paginator="true" rows="15" rowIndexVar="index" resizableColumns="true"
                                 emptyMessage="No se encontraron resultados">
                        <p:column headerText="Ord." style="width:5%;text-align: center" sortBy="#{progr.sprvId}">
                            <h:outputText value="#{index + 1}" />
                        </p:column>
                        <p:column headerText="Nombre" style="width:40%;white-space: pre-line" sortBy="#{progr.sprvNombre}">
                            <h:outputText  value ="#{progr.sprvNombre}"/> 
                        </p:column>
                        <p:column headerText="Departamento Responsable" style="width:30%;white-space: pre-line" sortBy="#{progr.stvsubjCode.stvsubjDesc}">
                            <h:outputText value="#{progr.stvsubjCode.stvsubjDesc}"/>
                        </p:column>
                        <p:column headerText="Seleccione" style="width: 5%; text-align: center">
                            <p:commandButton id="btnSeleccione" icon="ui-icon-circle-check" style="margin: 0;font-size: 11px;"
                                             title="Seleccione Programa" action="#{programaVinculacionController.seleccionaPrograma()}"
                                             update=":formPrincipal:formPrincipal1:panelBase" process="@this">
                                <f:setPropertyActionListener value="#{progr}" target="#{programaVinculacionController.programaSelected}"/>
                            </p:commandButton>
                        </p:column>
                    </p:dataTable>
                </p:panel>




                <p:tabView id="tabViewPrincipal" activeIndex="#{programaVinculacionController.indiceTabActivo}" scrollable="false" rendered="#{!programaVinculacionController.botonInicio}" style="width: 100%; height: 100%" dynamic="true" effect="fold" effectDuration="fast">
                    <!--tab: Planificacion-->
                    <p:tab title="Planificación" >




                        <p:tabView id="tabP" activeIndex="#{programaVinculacionController.indiceTabActivo}" scrollable="false" orientation="left" rendered="#{!programaVinculacionController.botonInicio}" style="width: 100%; height: 100%" dynamic="true" effect="slide" effectDuration="fast">
                            <p:ajax event="tabChange" 
                                    update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:p1,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:p2,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h2, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:vinPCobertura,   :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h4, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:p5,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h5, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:p6,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h6, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:p7,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h7, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h8, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:pInput8, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h9, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:pInput9, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:p10, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h10, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:p11, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h11, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h12, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:pInput12, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h13, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:pInput13, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h15, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:p15, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:p16, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h16, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:dataTableObjParroquiales"
                                    listener="#{programaVinculacionController.changeListener}"></p:ajax>



                            <!--tab1: Datos Generales-->
                            <p:tab title="Datos Generales" id="tabP1">
                                <p:panel id="p1"> 
                                    <h:panelGrid columns="3" cellpadding="7" id="h1">
                                        <p:commandButton id="btnGrabarP1" value="Grabar" icon="ui-icon-locked" style="margin: 0; font-size: 11px; float: left" disabled="#{programaVinculacionController.validaVaciosP1 || programaVinculacionController.programaSelected.sprvFechaFinReal!=null}"
                                                         title="Graba Información" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h1,:formPrincipal:formPrincipal1:panelBase,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:pInput1" action="#{programaVinculacionController.registraP1()}">
                                        </p:commandButton>
                                        <p:commandButton value="Eliminar" icon="ui-icon-closethick" style="margin: 0; font-size: 11px; float: left" disabled="#{programaVinculacionController.programaSelected.sprvId==null}"
                                                         title="Eliminar Programa" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h1,:formPrincipal:formPrincipal1:panelBase,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:pInput1"
                                                         onclick="PF('dialogDeletePrograma').show();" process="@this"
                                                         >
                                        </p:commandButton>                                        
                                        <p:commandButton value="Imprimir" icon="ui-icon-print" style="margin: 0; font-size: 11px; float: left" disabled="#{programaVinculacionController.programaSelected.sprvId==null}"
                                                         title="Imprimir Programa" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h1,:formPrincipal:formPrincipal1:panelBase,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:pInput1"
                                                         process="@this"
                                                         >
                                        </p:commandButton>
                                    </h:panelGrid>
                                    <h:panelGrid columns="2" cellpadding="2"  id="pInput1"  style="text-align: left">
                                        <h:outputLabel for="codProg" value="Código:" style="text-align: left"/>
                                        <h:outputLabel id="codProg" value="ABC000" style="text-align: left"/>

                                        <h:outputLabel for="nombProg" value="Nombre del Programa:" style="text-align: left"/>
                                        <p:inputTextarea id="nombProg" value="#{programaVinculacionController.nombreProg}" maxlength="150" rows="2" cols="80" required="false">
                                            <p:ajax event="keyup" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP1" listener="#{programaVinculacionController.validarVaciosP1}"/>
                                        </p:inputTextarea>

                                    </h:panelGrid>
                                    <h:panelGrid columns="2" id="panelNuevoCampus">
                                        <h:outputLabel  value="Campus:" style="text-align: left"/>
                                        <p:commandButton id="btnNuevoCampus" value="Agregar" icon="ui-icon-plus" style="margin: 0; font-size: 11px; float: left"
                                                         title="Agregar Campus" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panelNuevoCampus, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panelIngresoCampus" action="#{programaVinculacionController.nuevoCampus()}" rendered="#{!programaVinculacionController.visualizarIngresoCampus}">
                                        </p:commandButton>
                                    </h:panelGrid>
                                    <p:panel id="panelIngresoCampus" visible="#{programaVinculacionController.visualizarIngresoCampus}" style="font-size: 11px; text-align: left; border: hidden; font-weight: bold">
                                        <h:panelGrid columns="2" cellpadding="5" cellspacing="0">
                                            <p:commandButton id="btnAgregarCampus" value="Aceptar" icon="ui-icon-locked" style="margin: 0; font-size: 11px; float: left " disabled="#{programaVinculacionController.habilitarAgregarCampus}"
                                                             title="Graba Información" action="#{programaVinculacionController.agregarCampus()}" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP1, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panelNuevoCampus,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panelIngresoCampus,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:dataTableCampus, :formPrincipal:formPrincipal1:message">
                                            </p:commandButton>
                                            <p:commandButton value="Cancelar" icon="ui-icon-arrowreturnthick-1-w" style="margin: 0; font-size: 11px; float: left "
                                                             title="Cancela Información" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panelNuevoCampus, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panelIngresoCampus" action="#{programaVinculacionController.cancelarCampus}">
                                            </p:commandButton>
                                        </h:panelGrid>                         

                                        <p:selectOneMenu value="#{programaVinculacionController.codCampus}" required="false" style="width: 250px">
                                            <f:selectItem itemLabel="[[-Seleccione un item-]]" itemValue="" noSelectionOption="true"/>
                                            <f:selectItems value="#{programaVinculacionController.campusList}" var="cam" itemLabel="#{cam.stvcampDesc}" itemValue="#{cam.stvcampCode}"/>
                                            <p:ajax event="change" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnAgregarCampus" listener="#{programaVinculacionController.seleccionarCampus()}"/>
                                        </p:selectOneMenu>

                                    </p:panel>                                    


                                    <p:dataTable id="dataTableCampus" value="#{programaVinculacionController.programaSelected.campusList}" var="campus"
                                                 style="width: 400px" paginator="false" rows="10" rowIndexVar="index"  resizableColumns="true" 
                                                 emptyMessage="No se encontraron resultados">
                                        <p:column headerText="Ord." width="20">
                                            <h:outputText value="#{index + 1}"/>
                                        </p:column>
                                        <p:column headerText="Código" width="40" style="white-space:pre-line" >
                                            <h:outputText value="#{campus.campus.stvcampCode}" style="text-transform: uppercase; white-space: pre-line; "/>
                                        </p:column>
                                        <p:column headerText="Nombre" style="white-space:pre-line" >
                                            <h:outputText value="#{campus.campus.stvcampDesc}" style="text-transform: uppercase; white-space: pre-line; "/>
                                        </p:column>      
                                        <p:column headerText="Eliminar" width="40" style="text-align: center">
                                            <p:commandButton style="margin:0" title="Eliminar el registro" icon="ui-icon-trash" 
                                                             action="#{programaVinculacionController.eliminarCampus()}"
                                                             update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:dataTableCampus, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP1">
                                                <f:setPropertyActionListener value="#{campus}" target="#{programaVinculacionController.svinDepcenproySelected}"/>
                                            </p:commandButton>

                                        </p:column>
                                    </p:dataTable>

                                    <p:spacer height="50" width="1"></p:spacer>

                                    <h:outputText value="PLAZO DE EJECUCIÓN" style="alignment-adjust: middle;font-size: small; font-weight: bold "/>
                                    <h:panelGrid  columns="2" cellpadding="2" >
                                        <p:outputLabel for="fechInicio" value="Fecha de Inicio (dd/mm/aaaa):" style="text-align: left"/>
                                        <p:calendar id="fechInicio" navigator="true" value="#{programaVinculacionController.fechaInicio}" yearRange="2015:2030" size="10" style="font-size: 11px" locale="es" pattern="dd/MM/yyyy">
                                            <p:ajax event="dateSelect" listener="#{programaVinculacionController.calculaDuracion}" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP" />
                                        </p:calendar>
                                        <p:outputLabel for="fechFin" value="Fecha de Finalización (dd/mm/aaaa):" style="text-align: left"/>
                                        <p:calendar id="fechFin" navigator="true" value="#{programaVinculacionController.fechaFin}" yearRange="2015:2030" size="10" style="font-size: 11px" locale="es" pattern="dd/MM/yyyy">
                                            <p:ajax event="dateSelect" listener="#{programaVinculacionController.calculaDuracion}" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP" />
                                        </p:calendar>
                                        <h:outputLabel for="dur" value="Duración:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;" style="text-align: left"/>
                                        <h:outputLabel id="dur" value="#{programaVinculacionController.duracion}"/>                 

                                        <p:spacer height="15"></p:spacer>
                                    </h:panelGrid>


                                    <h:panelGrid columns="1">
                                        <h:outputText value="DATOS DEL DIRECTOR" style="alignment-adjust: middle;font-size: small; font-weight: bold;  "/>
                                        <h:outputText value="(Ingrese nombres y apellidos a buscar)"/>
                                        <p:autoComplete minQueryLength="3" value="#{programaVinculacionController.nomDocenteBusqueda}" completeMethod="#{programaVinculacionController.completeText}" size="70">
                                            <p:ajax event="itemSelect" listener="#{programaVinculacionController.onItemSelect}"  update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP1, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:datosResp"/>
                                        </p:autoComplete>
                                    </h:panelGrid>
                                    <h:panelGrid id="datosResp"  columns="2" cellpadding="2" > 
                                        <h:outputLabel value="Nombres:" style="text-align: left"/>
                                        <p:outputLabel value="#{programaVinculacionController.docente.usuNombres}" style="width:300px; font: xx-small; font-weight: normal"/>

                                        <h:outputLabel value="Apellidos:" style="text-align: left"/>
                                        <p:outputLabel value="#{programaVinculacionController.docente.usuApellidos}" style="width:300px; font: xx-small; font-weight: normal"/>

                                        <h:outputLabel value="Cargo:" style="text-align: left"/>
                                        <p:outputLabel value="#{programaVinculacionController.docente.cargo}" style="width:300px; font: xx-small; font-weight: normal"/>

                                        <h:outputLabel value="Departamento:" style="text-align: left"/>
                                        <p:outputLabel value="#{programaVinculacionController.docente.departamento}" style="width:300px; font: xx-small; font-weight: normal"/>

                                        <h:outputLabel value="Correo Electrónico:" style="text-align: left"/>
                                        <p:inputText value="#{programaVinculacionController.docente.usuEmail}" maxlength="50" required="false" disabled="false" style="width: 292px"> 
                                            <p:ajax event="keyup" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP1" listener="#{programaVinculacionController.validarVaciosP1}"/>
                                        </p:inputText>
                                        <p:outputLabel for="telefUsu" value="Teléfono:" style="white-space: pre-line" /> 
                                        <p:inputText id="telefUsu" value="#{programaVinculacionController.docente.usuTelefonoFijo}" maxlength="10" autocomplete="off" required="false" disabled="false" style="width: 80px">
                                            <p:ajax event="keyup" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP1" listener="#{programaVinculacionController.validarVaciosP1}"/>
                                        </p:inputText>
                                    </h:panelGrid>                                    


                                </p:panel>
                            </p:tab>
                            <!--tab2: Entidad Ejecutora-->
                            <p:tab title="Entidad Ejecutora" id="tabP2" disabled="#{programaVinculacionController.habilitaTab}">
                                <h:panelGrid columns="1" cellspacing="0" cellpadding="5" id="h2">
                                    <p:commandButton id="btnAgregarP2" value="Agregar" icon="ui-icon-plus" style="margin: 0; font-size: 11px; float: left"
                                                     title="Agregar Información" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:p2,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h2" 
                                                     action="#{programaVinculacionController.agregarP2()}" rendered="#{!programaVinculacionController.controlGrabarP2}">
                                    </p:commandButton>

                                </h:panelGrid>
                                <p:panel id="p2" visible="#{programaVinculacionController.controlGrabarP2}" style="font-size: 11px; text-align: left; border: hidden; font-weight: bold">
                                    <h:panelGrid columns="2" cellpadding="5" cellspacing="0">
                                        <p:commandButton id="btnGrabarP2" value="Grabar" icon="ui-icon-locked" style="margin: 0; font-size: 11px; float: left " disabled="#{programaVinculacionController.validaVaciosP2}"
                                                         title="Graba Información" action="#{programaVinculacionController.registraP2()}" 
                                                         update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:p2,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h2,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:dataTableProgVin, :formPrincipal:formPrincipal1:message">
                                        </p:commandButton>
                                        <p:commandButton id="btnCancelarP2" value="Cancelar" icon="ui-icon-arrowreturnthick-1-w" style="margin: 0; font-size: 11px; float: left "
                                                         title="Cancela Información" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:p2,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h2" action="#{programaVinculacionController.cancelarP2}">
                                        </p:commandButton>
                                    </h:panelGrid>                         
                                    <h:panelGrid columns="2" cellpadding="5">
                                        <h:outputLabel for="ejecPrinc" value="Unidad Ejecutora2:" style="text-align: left"/>
                                        <p:inputText id="ejecPrinc" value="#{programaVinculacionController.entidadEjecutoraSelected.sopinNombre}" required="false" style="width: 450px" maxlength="100">
                                            <p:ajax event="keyup" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP2" listener="#{programaVinculacionController.validarVaciosP2}"/>
                                        </p:inputText>                            

                                        <p:outputLabel for="direc" value="Dirección:" style="text-align: left"/>
                                        <p:inputText id="direc" value="#{programaVinculacionController.entidadEjecutoraSelected.sopinDireccion}" required="false" style="width: 450px" maxlength="100">
                                            <p:ajax event="keyup" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP2" listener="#{programaVinculacionController.validarVaciosP2}"/>
                                        </p:inputText>

                                        <h:outputLabel for="prov" value="Provincia:"/>
                                        <p:selectOneMenu id="prov" value="#{programaVinculacionController.codProvinciaSelected}" style="width:250px;"
                                                         >
                                            <f:selectItem itemLabel="[[-Seleccione un item-]]" itemValue="" noSelectionOption="true"/>
                                            <f:selectItems value="#{programaVinculacionController.provinciaList}" var="provin" itemLabel="#{provin.proNombre}" itemValue="#{provin.proId}"/>
                                            <p:ajax event="change" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP2" listener="#{programaVinculacionController.validarVaciosP2}" />
                                        </p:selectOneMenu>
                                        <h:outputLabel for="ciud" value="Ciudad:"/>

                                        <h:panelGrid columns="6" cellpadding="5" cellspacing="0" id="pInput2" style="text-align: left">

                                            <p:inputText id="ciud" value="#{programaVinculacionController.entidadEjecutoraSelected.sopinCiudad}" required="false" maxlength="25">
                                                <p:ajax event="keyup" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP2" listener="#{programaVinculacionController.validarVaciosP2}"/>
                                            </p:inputText>
                                            <h:outputLabel for="pagWeb" value="Página Web:" style="text-align: left;"/>
                                            <p:inputText id="pagWeb" value="#{programaVinculacionController.entidadEjecutoraSelected.sopinPaguinaWeb}" required="false" style="width: 200px" maxlength="25">
                                                <p:ajax event="keyup" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP2" listener="#{programaVinculacionController.validarVaciosP2}"/>
                                            </p:inputText>
                                            <p:outputLabel for="telef" value="Teléfono:" style="white-space: pre-line"/>
                                            <p:inputMask id="telef" value="#{programaVinculacionController.entidadEjecutoraSelected.sopinTelefono}" required="false" mask="(999)99-9999999">
                                                <p:ajax event="keyup" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP2" listener="#{programaVinculacionController.validarVaciosP2}"/>
                                            </p:inputMask>
                                        </h:panelGrid>
                                    </h:panelGrid>
                                </p:panel>
                                <h:outputLabel value="Entidad ejecutora:" style="text-align: left"/>
                                <p:dataTable id="dataTableProgVin" value="#{programaVinculacionController.entidadEjecutoraList}" var="progVin"
                                             style="width: 100%" paginator="true" rows="10" rowIndexVar="index"  resizableColumns="true" 
                                             emptyMessage="No se encontraron resultados">
                                    <p:column headerText="Ord." style="width:20px">
                                        <h:outputText value="#{index + 1}"/>
                                    </p:column>
                                    <p:column headerText="Nombre" style="white-space:pre-line" >
                                        <h:outputText value="#{progVin.sopinNombre}" style="text-transform: uppercase; white-space: pre-line; "/>
                                    </p:column>
                                    <p:column headerText="Teléfono" style="width: 80px;white-space:pre-line" >
                                        <h:outputText value="#{progVin.sopinTelefono}" style="text-transform: uppercase; white-space: pre-line; "/>
                                    </p:column>     
                                    <p:column headerText="Dirección" style="width: 100px;white-space:pre-line" >                               
                                        <h:outputText value="#{progVin.sopinDireccion}" style="text-transform: uppercase; white-space: pre-line; width: 100%"/>
                                    </p:column>
                                    <p:column headerText="Ciudad" style="width: 100px;white-space:pre-line" >
                                        <h:outputText value="#{progVin.sopinCiudad}" style="text-transform: uppercase; white-space: pre-line; width: 100%"/>
                                    </p:column> 
                                    <p:column headerText="Provincia" style="width:100px">
                                        <h:outputText value="#{progVin.proId.proNombre}" style="text-transform: uppercase; white-space: pre-line; width: 100%"/>
                                    </p:column>
                                    <p:column headerText="Página Web" style="width: 100px;white-space:pre-line" >
                                        <h:outputText value="#{progVin.sopinPaguinaWeb}" style="text-transform: uppercase; white-space: pre-line; width: 100%"/>
                                    </p:column>                           
                                    <p:column headerText="Editar" style="width: 40px; text-align: center">
                                        <p:commandButton icon="ui-icon-pencil"
                                                         title="Editar el registro" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:p2,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h2" action="#{programaVinculacionController.seleccionarP2()}"
                                                         oncomplete="document.getElementById('formPrincipal:formPrincipal1:tabViewPrincipal:tabP:ejecPrinc').focus();">
                                            <f:setPropertyActionListener value="#{progVin}" target="#{programaVinculacionController.entidadEjecutoraSelected}"/>
                                        </p:commandButton>
                                    </p:column>
                                    <p:column headerText="Eliminar" style="width: 40px;text-align: center">
                                        <p:commandButton id="btnEliminarP2" style="margin: 0" title="Eliminar el registro" icon="ui-icon-trash" 
                                                         onclick="PF('dialogDeleteEntidadEjecutora').show();" process="@this">
                                            <f:setPropertyActionListener value="#{progVin}" target="#{programaVinculacionController.entidadEjecutoraSelected}"/>
                                        </p:commandButton>
                                    </p:column>
