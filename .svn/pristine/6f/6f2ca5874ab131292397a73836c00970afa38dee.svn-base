                                        </p:column>
                                        <p:column headerText="Semestre" width="100" sortBy="#{hitoProyecto.semestre.prdDescripcion}">
                                            <h:outputText value="#{hitoProyecto.semestre.prdDescripcion}" style="white-space: pre-line"/>                                    
                                        </p:column>
