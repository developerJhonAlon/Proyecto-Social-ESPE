                                                <p:ajax event="change" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP7, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnCancelarP7"/>
                                            </p:selectOneMenu>

                                            <h:outputLabel value="Descripción:"/>
