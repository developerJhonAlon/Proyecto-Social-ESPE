                                        <p:column headerText="Valor" sortBy="#{hitoProyecto.valor}" width="100">
                                            <h:outputText value="#{hitoProyecto.valor}" style="white-space: pre-line"/>     
                                        </p:column>

                                        <p:column headerText="Editar" width="40" style="text-align: center">
