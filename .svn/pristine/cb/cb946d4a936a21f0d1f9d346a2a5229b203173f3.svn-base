<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<ui:composition
    xmlns:ui="http://java.sun.com/jsf/facelets"
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:f="http://java.sun.com/jsf/core"
    xmlns:h="http://java.sun.com/jsf/html"
    template="/vista/plantilla.xhtml"
    xmlns:p="http://primefaces.org/ui"
    xmlns:c="http://java.sun.com/jsp/jstl/core">
    <ui:param name="backingBean" value="#{programaVinculacionController}"/>
    <ui:param name="tituloPagina" value=""/>
    <ui:define name="editar"/> 
    <ui:define name="contenido">
        <h:form id="formPrincipal1" enctype="multipart/form-data">
            <p:panel id="panelBase" header="Programas Vinculación" style="font-size: 11px; text-align: left; border: hidden; font-weight: bold">
                <h:panelGrid columns="2" id="hpCabecera" >
                    <p:selectBooleanButton id="value2" value="#{programaVinculacionController.botonInicio}" onLabel="Agregar" offLabel="Programas Vinculación" onIcon="ui-icon-plus" offIcon="ui-icon-arrowreturnthick-1-w" style="width:100px">
                        <p:ajax update=":formPrincipal:formPrincipal1:panelBase,:formPrincipal:formPrincipal1:tabP:nombProg" listener="#{programaVinculacionController.inicioProgramas}"/>
                    </p:selectBooleanButton>
                </h:panelGrid>
                <p:panel id="inicio" visible="#{programaVinculacionController.botonInicio}" style="font-size: 11px; text-align: left; border: hidden; font-weight: bold">
                    <p:dataTable id="dtProgramaVinculacion" value="#{programaVinculacionController.vinProgrList}" var="progr" paginator="true" rows="15" rowIndexVar="index" resizableColumns="true"
                                 emptyMessage="No se encontraron resultados">
                        <p:column headerText="Ord." style="width:5%;text-align: center">
                            <h:outputText value="#{index + 1}" />
                        </p:column>
                        <p:column headerText="Nombre" style="width:40%;white-space: pre-line">
                            <h:outputText  value ="#{progr.sprvNombre}"/> 
                        </p:column>
                        <p:column headerText="Departamento Responsable" style="width:30%;white-space: pre-line">
                            <h:outputText value="#{progr.stvsubjCode.stvsubjDesc}"/>
                        </p:column>
                        <p:column headerText="Seleccione" style="width: 5%; text-align: center">
                            <p:commandButton id="btnSeleccione" icon="ui-icon-circle-check" style="margin: 0;font-size: 11px;"
                                             title="Seleccione Programa" action="#{programaVinculacionController.seleccionaPrograma()}"
                                             update=":formPrincipal:formPrincipal1:panelBase" process="@this">
                                <f:setPropertyActionListener value="#{progr}" target="#{programaVinculacionController.vinProgramaVSelected}"/>
                            </p:commandButton>
                        </p:column>
                    </p:dataTable>
                </p:panel>
                <p:tabView id="tabP" scrollable="false" orientation="left" rendered="#{!programaVinculacionController.botonInicio}" style="width: 100%; height: 100%" dynamic="true" effect="slide" effectDuration="slow">
                    <p:tab title="Datos Generales" id="tabP1">
                        <h:panelGrid columns="1" cellpadding="7" id="h1">
                            <p:commandButton id="btnGrabarP1" value="Grabar" icon="ui-icon-locked" style="margin: 0; font-size: 11px; float: left" disabled="#{programaVinculacionController.validaVaciosP1}"
                                             title="Graba Información" update=":formPrincipal:formPrincipal1:tabP:h1,:formPrincipal:formPrincipal1:tabP,:formPrincipal:formPrincipal1:tabP,:formPrincipal:formPrincipal1:tabP:pInput1" action="#{programaVinculacionController.registraP1()}">
                            </p:commandButton>
                        </h:panelGrid>
                        <h:panelGrid columns="2" cellpadding="5"  id="pInput1"  style="text-align: left">
                            <h:outputLabel for="nombProg" value="Nombre del Programa:" style="text-align: left"/>
                            <p:inputTextarea id="nombProg" value="#{programaVinculacionController.nombreProg}" rows="2" cols="80" required="false">
                                <p:ajax event="keyup" update=":formPrincipal:formPrincipal1:tabP:btnGrabarP1" listener="#{programaVinculacionController.validarVaciosP1}"/>
                            </p:inputTextarea>
                            <h:outputLabel  for="depResp" value="Nombre del Departamento Responsable:" style="text-align: left"/>
                            <p:selectOneMenu id="depResp" value="#{programaVinculacionController.depaResp}" required="false" style="width: 75%">
                                <f:selectItem itemLabel="Seleccione Departamento" itemValue="" noSelectionOption="true"/>
                                <f:selectItems value="#{programaVinculacionController.stvSubjList}" var="dep" itemLabel="#{dep.stvsubjDesc}" itemValue="#{dep.stvsubjCode}"/>
                                <p:ajax event="change" update=":formPrincipal:formPrincipal1:tabP:nomApellidos" listener="#{programaVinculacionController.cargaDatosPersonas}"/>
                            </p:selectOneMenu>
                        </h:panelGrid>

                        <h:panelGrid  columns="2" cellpadding="5" >
                            <h:outputText value="PLAZO DE EJECUCIÓN" style="alignment-adjust: middle;font-size: small; background: #4EA30D; "/>
                            <h:outputText value=""/>
                            <h:outputText value=""/>
                            <h:outputText value=""/>
                            <p:outputLabel for="fechInicio" value="Fecha de Inicio (dd/mm/aaaa):&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;" style="text-align: left"/>
                            <p:calendar id="fechInicio" navigator="true" value="#{programaVinculacionController.fechaInicio}" yearRange="2015:2030" size="10" style="font-size: 11px" locale="es" pattern="dd/MM/yyyy">
                                <p:ajax event="dateSelect" listener="#{programaVinculacionController.calculaDuracion}" update=":formPrincipal:formPrincipal1:tabP" />
                            </p:calendar>
                            <p:outputLabel for="fechFin" value="Fecha de Finzalización (dd/mm/aaaa):&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;" style="text-align: left"/>
                            <p:calendar id="fechFin" navigator="true" value="#{programaVinculacionController.fechaFin}" yearRange="2015:2030" size="10" style="font-size: 11px" locale="es" pattern="dd/MM/yyyy">
                                <p:ajax event="dateSelect" listener="#{programaVinculacionController.calculaDuracion}" update=":formPrincipal:formPrincipal1:tabP" />
                            </p:calendar>
                            <h:outputLabel for="dur" value="Duración:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;" style="text-align: left"/>
                            <h:outputLabel id="dur" value="#{programaVinculacionController.duracion}"/>                 

                        </h:panelGrid>
                        <h:panelGrid  columns="4" cellpadding="5" >
                            <h:outputText value="DATOS DEL RESPONSABLE" style="alignment-adjust: middle;font-size: small; background: #4EA30D; "/>
                            <h:outputText value=""/>
                            <h:outputText value=""/>
                            <h:outputText value=""/>
                            <h:outputLabel for="nomApellidos" value="Nombres y Apellidos:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;" style="text-align: left"/>
                            <p:selectOneMenu id="nomApellidos" value="#{programaVinculacionController.codDocente}" required="false" style="width:300px; font: xx-small">
                                <f:selectItem itemLabel="Seleccione el nombre del Responsable" itemValue="" noSelectionOption="true"/>
                                <f:selectItems value="#{programaVinculacionController.doceVincuLista}" var="doc"  itemLabel="#{doc.usuApellidos} -  #{doc.usuNombres}" itemValue="#{doc.usuId}"/>
                                <p:ajax event="change" update=":formPrincipal:formPrincipal1:tabP:mail,:formPrincipal:formPrincipal1:tabP:telefUsu,:formPrincipal:formPrincipal1:tabP:cargo,:formPrincipal:formPrincipal1:tabP:btnGrabarP1" listener="#{programaVinculacionController.cargaDatosContacto()}" />
                            </p:selectOneMenu>
                            <h:outputLabel for="cargo" value="Cargo:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;" style="text-align: left"/>
                            <p:inputText id="cargo" value="#{programaVinculacionController.cargodocVinculacion.docente}" required="false" style="width:250px; font: xx-small">
                                <p:ajax event="change" update=":formPrincipal:formPrincipal1:tabP:btnGrabarP1" />
                            </p:inputText>
                            <h:outputLabel for="mail" value="Correo Electronico:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;" style="text-align: left"/>
                            <p:inputText id="mail" value="#{programaVinculacionController.docente.usuEmail}" required="false" disabled="true">
                                <p:ajax event="keyup" update=":formPrincipal:formPrincipal1:tabP:btnGrabarP1"/>
                            </p:inputText>
                            <p:outputLabel for="telefUsu" value="Teléfono:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;" style="white-space: pre-line" /> 
                            <p:inputText id="telefUsu" value="#{programaVinculacionController.docente.usuTelefonoFijo}" autocomplete="off" required="false" disabled="true">
                                <p:ajax event="keyup" update=":formPrincipal:formPrincipal1:tabP:btnGrabarP1"/>
                            </p:inputText>
                        </h:panelGrid>
                    </p:tab>
                    <p:tab title="Entidad Ejecutora" id="tabP2" disabled="#{programaVinculacionController.habilitaTab}">
                        <h:panelGrid columns="1" cellspacing="0" cellpadding="5" id="h2">
                            <p:commandButton id="btnAgregarP2" value="Agregar" icon="ui-icon-plus" style="margin: 0; font-size: 11px; float: left"
                                             title="Agregar Información" update=":formPrincipal:formPrincipal1:tabP:p2,:formPrincipal:formPrincipal1:tabP:h2" action="#{programaVinculacionController.agregarP2()}" rendered="#{!programaVinculacionController.controlGrabarP2}">
                                <!--title="Graba Información" update=":formPrincipal:formPrincipal1:tabP:p8,:formPrincipal:formPrincipal1:tabP:h8,:formPrincipal:formPrincipal1:tabP:dataTableFinanProy,:formPrincipal:formPrincipal1:tabP:pInput8" action="#{perfilProyectoController.registraP8()}">-->
                            </p:commandButton>

                        </h:panelGrid>
                        <p:panel id="p2" visible="#{programaVinculacionController.controlGrabarP2}" style="font-size: 11px; text-align: left; border: hidden; font-weight: bold">
                            <h:panelGrid columns="2" cellpadding="5" cellspacing="0">
                                <p:commandButton id="btnGrabarP2" value="Grabar" icon="ui-icon-locked" style="margin: 0; font-size: 11px; float: left " disabled="#{programaVinculacionController.validaVaciosP2}"
                                                 title="Graba Información" action="#{programaVinculacionController.registraP2()}">
                                </p:commandButton>
                                <p:commandButton id="btnCancelarP2" value="Cancelar" icon="ui-icon-arrowreturnthick-1-w" style="margin: 0; font-size: 11px; float: left "
                                                 title="Cancela Información" update=":formPrincipal:formPrincipal1:tabP:p2,:formPrincipal:formPrincipal1:tabP:h2" action="#{programaVinculacionController.cancelarP2}">
                                </p:commandButton>
                            </h:panelGrid>                         
                            <h:panelGrid columns="2" cellpadding="5">
                                <h:outputLabel for="ejecPrinc" value="Institución Ejecutora Principal:" style="text-align: left"/>
                                <p:inputText id="ejecPrinc" value="#{programaVinculacionController.instEjecutora}" required="false" style="width: 450px">
                                    <p:ajax event="keyup" update=":formPrincipal:formPrincipal1:tabP:btnGrabarP2" listener="#{programaVinculacionController.validarVaciosP2}"/>
                                </p:inputText>                            

                                <p:outputLabel for="direc" value="Dirección:" style="text-align: left"/>
                                <p:inputText id="direc" value="#{programaVinculacionController.direccion}" required="false" style="width: 450px">
<!--                                <p:inputText id="direc" value="#{programaVinculacionController.direccion}" autocomplete="off" required="false" size="100" maxlength="100">-->
                                    <p:ajax event="keyup" update=":formPrincipal:formPrincipal1:tabP:btnGrabarP2" listener="#{programaVinculacionController.validarVaciosP2}"/>
                                </p:inputText>
                                <h:outputLabel for="prov" value="Provincia:" style="text-align: left"/>
                                <p:selectOneMenu id="prov" value="#{programaVinculacionController.provinciaEntidad}" required="false" style="width:250px; font: xx-small">
                                    <f:selectItem itemLabel="Seleccione el nombre de la Provincia" itemValue="" noSelectionOption="true"/>
                                    <f:selectItems value="#{programaVinculacionController.seaProvinciaList}" var="provin" itemLabel="#{provin.proNombre}" itemValue="#{provin.proId}"/>
                                    <p:ajax event="change" update=":formPrincipal:formPrincipal1:tabP:btnGrabarP2" listener="#{programaVinculacionController.validarVaciosP2}"/>
                                    <h:outputLabel value=""/>
                                </p:selectOneMenu>
                            </h:panelGrid>
                            <h:panelGrid columns="6" cellpadding="5" cellspacing="0" id="pInput2" style="text-align: left">

                                <h:outputLabel for="ciud" value="Ciudad:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;" style="text-align: left"/>
                                <p:inputText id="ciud" value="#{programaVinculacionController.ciudad}" required="false">
                                    <p:ajax event="keyup" update=":formPrincipal:formPrincipal1:tabP:btnGrabarP2" listener="#{programaVinculacionController.validarVaciosP2}"/>
                                </p:inputText>
                                <h:outputLabel for="pagWeb" value="Pagina Web:" style="text-align: left"/>
                                <p:inputText id="pagWeb" value="#{programaVinculacionController.pagWeb}" required="false">
                                    <p:ajax event="keyup" update=":formPrincipal:formPrincipal1:tabP:btnGrabarP2" listener="#{programaVinculacionController.validarVaciosP2}"/>
                                </p:inputText>
                                <p:outputLabel for="telef" value="Teléfono:" style="white-space: pre-line"/>
                                <p:inputMask id="telef" value="#{programaVinculacionController.telefono}" required="false" mask="(999)99-9999999">
                                    <p:ajax event="keyup" update=":formPrincipal:formPrincipal1:tabP:btnGrabarP2" listener="#{programaVinculacionController.validarVaciosP2}"/>
                                </p:inputMask>
                                <h:outputLabel value="Anexos:"/>
                                <h:outputLabel value="#{programaVinculacionController.nomFile2}"/>
                            </h:panelGrid>
                            <left>
                                <p:fileUpload fileUploadListener="#{programaVinculacionController.handleFileUpload2}" mode="advanced" dragDropSupport="false" style="width: 250px; text-align: left"
                                              update=":formPrincipal:formPrincipal1:tabP:btnGrabarP2" label="Seleccione Documento" sizeLimit="100000" auto="true"/>
                            </left>
                        </p:panel>
                        <p:dataTable id="dataTableProgVin" value="#{programaVinculacionController.opcInstitucionList}" var="progVin"
                                     editable="true" style="width: 100%" paginator="false" rows="10" rowIndexVar="index"  resizableColumns="true" 
                                     emptyMessage="No se encontraron resultados">
                            <p:ajax event="rowEdit" listener="#{programaVinculacionController.onRowEditP2}" partialSubmit="true"/>
                            <p:ajax event="rowEditCancel" listener="#{programaVinculacionController.onRowCancelP2}" partialSubmit="true"/>

                            <p:column headerText="Ord." style="width:2%">
                                <h:outputText value="#{index + 1}"/>
                            </p:column>
                            <p:column headerText="Nombre Institución Ejecutora" style="width: 5%;white-space:pre-line" >
                                <p:cellEditor> 
                                    <f:facet name="output"><h:outputText value="#{progVin.sopinOrganoEjecutor}" style="text-transform: uppercase; white-space: pre-line; width: 100%"/></f:facet>
                                    <f:facet name="input"><p:inputText id="modelInput21" value="#{progVin.sopinOrganoEjecutor}" style="width: 100%" /></f:facet>
                                </p:cellEditor>                              
                            </p:column>
                            <p:column headerText="Teléfonos" style="width: 7%;white-space:pre-line" >
                                <p:cellEditor>
                                    <f:facet name="output"><h:outputText value="#{progVin.sopinTelefono}" style="text-transform: uppercase; white-space: pre-line; width: 100%"/></f:facet>
                                    <f:facet name="input"><p:inputText id="modelInput22" value="#{progVin.sopinTelefono}" style="width: 100%" /></f:facet>                                  
                                </p:cellEditor>                               
                            </p:column>     
                            <p:column headerText="Dirección" style="width: 5%;white-space:pre-line" >                               
                                <p:cellEditor>
                                    <f:facet name="output"><h:outputText value="#{progVin.sopinDireccion}" style="text-transform: uppercase; white-space: pre-line; width: 100%"/></f:facet>
                                    <f:facet name="input"><p:inputText id="modelInput23" value="#{progVin.sopinDireccion}" style="width: 100%" /></f:facet>                                  
                                </p:cellEditor> 
                            </p:column>
                            <p:column headerText="Ciudad" style="width: 5%;white-space:pre-line" >
                                <p:cellEditor>
                                    <f:facet name="output"><h:outputText value="#{progVin.sopinCiudad}" style="text-transform: uppercase; white-space: pre-line; width: 100%"/></f:facet>
                                    <f:facet name="input"><p:inputText id="modelInput24" value="#{progVin.sopinCiudad}" style="width: 100%" /></f:facet>                                  
                                </p:cellEditor> 
                            </p:column> 
                            <p:column headerText="Provincia" style="width:10%">
                                <p:cellEditor>
                                    <f:facet name="output"><h:outputText value="#{progVin.proId.proNombre}" style="text-transform: uppercase; white-space: pre-line"/> </f:facet>
                                    <f:facet name="input">
                                        <h:selectOneMenu value="#{programaVinculacionController.provinciaEdit}" style="width: 100%">
                                            <f:selectItems value="#{programaVinculacionController.seaProvinciaList}" var="provincia" itemLabel="#{provincia.proNombre}" itemValue="#{provincia.proId}"/>
                                        </h:selectOneMenu>
                                    </f:facet>
                                </p:cellEditor>
                            </p:column>
                            <p:column headerText="Página Web " style="width: 10%;white-space:pre-line" >
                                <p:cellEditor>
                                    <f:facet name="output"><h:outputText value="#{progVin.sopinPaguinaWeb}" style="text-transform: uppercase; white-space: pre-line; width: 100%"/></f:facet>
                                    <f:facet name="input"><p:inputText id="modelInput46" value="#{progVin.sopinPaguinaWeb}" style="width: 100%" /></f:facet>                                  
                                </p:cellEditor> 
                            </p:column>                           
                            <p:column headerText="Editar" style="width: 5%">
                                <p:rowEditor/>
                            </p:column>
                            <p:column headerText="Eliminar" style="width: 3%;text-align: center">
                                <p:commandButton id="btnEliminarP2" style="margin: 0" title="Eliminar el registro" icon="ui-icon-trash" 
                                                 onclick="PF('deleteDialog2').show();" process="@this">
                                    <f:setPropertyActionListener value="#{progVin}" target="#{programaVinculacionController.vinOpcInstitucionSelected}"/>
                                </p:commandButton>
                            </p:column>
                        </p:dataTable>
                    </p:tab>
                    <!--tab3-->
                    <p:tab title="Cobertura y Localización" id="tabP3" disabled="#{programaVinculacionController.habilitaTab}">
                        <h:panelGrid columns="1" cellpadding="5">
                            <p:commandButton id="btnGrabarP3" value="Grabar" icon="ui-icon-locked" style="margin: 0; font-size: 11px; float: left " 
                                             title="Graba Información" action="#{programaVinculacionController.registraP3()}">
                            </p:commandButton>
                        </h:panelGrid>
                        <h:panelGrid columns="1" cellpadding="5" cellspacing="0">
                            <p:selectOneRadio id="tipCobertura" value="#{programaVinculacionController.vinCoberturaNew.localId.localId}">
                                <f:selectItems value="#{programaVinculacionController.localizaList}" var="local" itemLabel="#{local.localDescrip}" itemValue="#{local.localId}"/>
                                <p:ajax event="change" update=":formPrincipal:formPrincipal1:tabP:vinPCobertura" listener="#{programaVinculacionController.validarVaciosP3}"/>
                            </p:selectOneRadio>
                        </h:panelGrid>
                        <p:panel id="vinPCobertura" style="font-size: 11px; text-align: left; border: hidden; font-weight: bold">
                            <h:panelGrid columns="2" id="vinPZonas" rendered="#{programaVinculacionController.vinCoberturaNew.localId.localId.toString()=='4'}">
                                <h:outputLabel for="vinZonasPlanificacion"/>
                                <p:selectManyCheckbox id="vinZonasPlanificacion" layout="grid" columns="1" value="#{programaVinculacionController.zonaPlafinicacion}">
                                    <f:selectItems value="#{programaVinculacionController.zonaPlanifList}" var="vinZona" itemLabel="#{vinZona.zopNombreLargo}" itemValue="#{vinZona.zopId}"/>
                                </p:selectManyCheckbox>
                            </h:panelGrid>
                            <h:panelGrid columns="2" id="vinProvincias" rendered="#{programaVinculacionController.vinCoberturaNew.localId.localId.toString()=='2'}">
                                <h:outputLabel for="vProvincia" />
                                <p:selectManyCheckbox id="vProvincia" layout="grid" columns="5" value="#{programaVinculacionController.provinciaProyecto}">
                                    <f:selectItems value="#{programaVinculacionController.provinciaList}" var="prov" itemLabel="#{prov.proNombre}" itemValue="#{prov.proId}"/>
                                </p:selectManyCheckbox>
                            </h:panelGrid>
                            <h:panelGrid columns="2" id="vinProvCanton" rendered="#{programaVinculacionController.vinCoberturaNew.localId.localId.toString()=='1'}">
                                <h:outputLabel for="vinProCanton"/>
                                <p:selectOneMenu id="vinProCanton" style="width: 220px" value="#{programaVinculacionController.vinZonaDetalleNew.canId.canId}">
                                    <f:selectItem itemLabel="Seleccione Provincia" itemValue="" noSelectionOption="true"/>
                                    <f:selectItems value="#{programaVinculacionController.provinciaList}" var="provCan" itemLabel="#{provCan.proNombre}" itemValue="#{provCan.proId}"/>
                                    <p:ajax event="change" update=":formPrincipal:formPrincipal1:tabP:vinCanton" />
                                </p:selectOneMenu>
                                <h:outputLabel for="vinCanton"/>
                                <p:selectManyCheckbox id="vinCanton" layout="grid" columns="5" value="#{programaVinculacionController.cantonProv}">
                                    <f:selectItems value="#{programaVinculacionController.cantonList}" var="canton" itemLabel="#{canton.canNombre}" itemValue="#{canton.canId}"/>
                                </p:selectManyCheckbox>
                            </h:panelGrid>
                        </p:panel>
                    </p:tab>
                    <!--tab4-->
                    <p:tab title="Objetivos del Plan Nacional del Buen Vivir" id="tabP4" disabled="#{programaVinculacionController.habilitaTab}">
                        <h:panelGrid columns="1" cellspacing="0" cellpadding="5" id="h4">
                            <p:commandButton id="btnAgregarP4" value="Agregar" icon="ui-icon-plus" style="margin: 0; font-size: 11px; float: left" disabled="#{programaVinculacionController.validaVaciosP4}"
                                             title="Agregar Información" update=":formPrincipal:formPrincipal1:tabP,:formPrincipal:formPrincipal1:tabP:dataTableObjPlan" action="#{programaVinculacionController.agregarP4()}" >
                            </p:commandButton>
                            <h:panelGrid columns="1" cellpadding="5">                                                       
                                <h:outputLabel for="objPlan" value="¿A qué objetivos del Plan Nacional del Buen Vivir apunta el Programa?" style="text-align: left"/>
                                <p:selectOneMenu id="objPlan" value="#{programaVinculacionController.objPadre.svobviId}" required="false" style="width:460px; font: xx-small">
                                    <f:selectItem itemLabel="Seleccione el Plan" itemValue="0" noSelectionOption="true"/>
                                    <f:selectItems value="#{programaVinculacionController.objBVivirPadreList}" var="objPadre" itemLabel="#{objPadre.svobviDescripcion}" itemValue="#{objPadre.svobviId}"/>
                                    <p:ajax event="change" update=":formPrincipal:formPrincipal1:tabP:subojPlan1" listener="#{programaVinculacionController.bsqSubObjetivos()}"/>
