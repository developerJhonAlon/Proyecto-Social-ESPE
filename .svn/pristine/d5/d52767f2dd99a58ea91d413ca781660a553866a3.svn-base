<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<ui:composition
    xmlns:ui="http://java.sun.com/jsf/facelets"
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:f="http://java.sun.com/jsf/core"
    xmlns:h="http://java.sun.com/jsf/html"
    template="/vista/plantilla.xhtml"
    xmlns:p="http://primefaces.org/ui"
    xmlns:c="http://java.sun.com/jsp/jstl/core"
    xmlns:pe="http://primefaces.org/ui/extensions"
    xmlns:o="http://omnifaces.org/ui"
    xmlns:of="http://omnifaces.org/functions"    
    >

    <ui:param name="backingBean" value="#{programaVinculacionController}"/>
    <ui:param name="tituloPagina" value=""/>
    <ui:define name="editar"/> 
    <ui:define name="contenido">
        <script type="text/javascript" src="resources/js/programaVinculacion.js"/>
        <style>
            .col1_entidadEjecutora {
                width: 70px;
            }
            .col1_actividad {
                width: 160px;
            }
            .col2_actividad {
                width: 160px;
            }
            .col3_actividad {
                width: 140px;
            }
            .col1_objetivoPrograma {
                width: 83px;
            }
            .ui-panel .ui-panel-content {
                padding: 0;
            }

        </style>
        <h:form id="formPrincipal1" enctype="multipart/form-data">
            <p:panel id="panelBase" header="Programas Vinculación" style="font-size: 11px; text-align: left; border: hidden; font-weight: bold">
                <p:growl id="message" showDetail="true" life="3000"></p:growl>            
                <h:panelGrid columns="3" id="hpCabecera" > 
                    <p:selectBooleanButton id="value2" value="#{programaVinculacionController.botonInicio}" immediate="false" onLabel="Agregar" offLabel="Programas Vinculación" onIcon="ui-icon-plus" offIcon="ui-icon-arrowreturnthick-1-w" style="width:100px">
                        <p:ajax update=":formPrincipal:formPrincipal1:panelBase" listener="#{programaVinculacionController.inicioProgramas}"/>
                    </p:selectBooleanButton>
                    <h:outputText  value ="#{programaVinculacionController.nombreProgSeleccionado}"/> 
                </h:panelGrid>
                <p:panel id="inicio" visible="#{programaVinculacionController.botonInicio}" style="font-size: 11px; text-align: left; border: hidden; font-weight: bold">
                    <p:dataTable id="dtProgramaVinculacion" value="#{programaVinculacionController.vinProgrList}" var="progr" paginator="true" rows="15" rowIndexVar="index" resizableColumns="true"
                                 emptyMessage="No se encontraron resultados">
                        <p:column headerText="Ord." style="width:5%;text-align: center" sortBy="#{progr.sprvId}">
                            <h:outputText value="#{index + 1}" />
                        </p:column>
                        <p:column headerText="Nombre" style="width:40%;white-space: pre-line" sortBy="#{progr.sprvNombre}">
                            <h:outputText  value ="#{progr.sprvNombre}"/> 
                        </p:column>
                        <!--<p:column headerText="Departamento Responsable" style="width:30%;white-space: pre-line" sortBy="#{progr.stvsubjCode.stvsubjDesc}">
                            <h:outputText value="#{progr.stvsubjCode.stvsubjDesc}"/>
                        </p:column>-->
                        <p:column headerText="Seleccione" style="width: 5%; text-align: center">
                            <p:commandButton id="btnSeleccione" icon="ui-icon-circle-check" style="margin: 0;font-size: 11px;"
                                             title="Seleccione Programa" action="#{programaVinculacionController.seleccionaPrograma()}"
                                             update=":formPrincipal:formPrincipal1:panelBase" process="@this">
                                <f:setPropertyActionListener value="#{progr}" target="#{programaVinculacionController.programaSelected}"/>
                            </p:commandButton>
                        </p:column>
                    </p:dataTable>
                </p:panel>




                <p:tabView id="tabViewPrincipal" activeIndex="#{programaVinculacionController.indiceTabActivo}" scrollable="false" rendered="#{!programaVinculacionController.botonInicio}" style="width: 100%; height: 100%" dynamic="true" effect="fold" effectDuration="fast">
                    <!--tab: Planificacion-->
                    <p:tab title="Planificación" >




                        <p:tabView id="tabP" activeIndex="#{programaVinculacionController.indiceTabActivo}" scrollable="false" orientation="left" rendered="#{!programaVinculacionController.botonInicio}" style="width: 100%; height: 100%" dynamic="true" effect="slide" effectDuration="fast">
                            <p:ajax event="tabChange" 
                                    process="@this"
                                    update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panel1,
                                    :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panel2,
                                    :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panel3,
                                    :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panel4, 
                                    :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panel5,
                                    :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panel6,
                                    :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panel7,
                                    :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panel8, 
                                    :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panel9, 
                                    :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panel10, 
                                    :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panel11, 
                                    :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panel12, 
                                    :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panel13, 
                                    :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panel15, 
                                    :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panel16, 
                                    :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panel17, 
                                    :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panel18"
                                    listener="#{programaVinculacionController.changeListener}"></p:ajax>



                            <!--tab1: Datos Generales-->
                            <p:tab title="Datos Generales" id="tabP1">
                                <p:panel id="panel1" style="border: hidden"> 
                                    <p:focus context="panel1"/>
                                    <h:panelGrid columns="3" cellpadding="7" id="h1">
                                        <p:commandButton id="btnGrabarP1" value="Grabar" icon="ui-icon-locked" style="margin: 0; font-size: 11px; float: left" disabled="#{programaVinculacionController.programaSelected.sprvFechaFinReal!=null}"
                                                         title="Graba Información" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h1,:formPrincipal:formPrincipal1:panelBase,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:pInput1" action="#{programaVinculacionController.registraP1()}"
                                                         onstart="PF('blockUIWidget').block()" oncomplete="PF('blockUIWidget').unblock()">
                                        </p:commandButton>
                                        <p:commandButton value="Eliminar" icon="ui-icon-closethick" style="margin: 0; font-size: 11px; float: left" disabled="#{programaVinculacionController.programaSelected.sprvId==null}"
                                                         title="Eliminar Programa" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h1,:formPrincipal:formPrincipal1:panelBase,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:pInput1"
                                                         onclick="PF('dialogDeletePrograma').show();" process="@this"
                                                         >
                                        </p:commandButton>                                        
                                        <p:commandButton value="Imprimir" rendered="false" icon="ui-icon-print" style="margin: 0; font-size: 11px; float: left" disabled="#{programaVinculacionController.programaSelected.sprvId==null}"
                                                         title="Imprimir Programa" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h1,:formPrincipal:formPrincipal1:panelBase,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:pInput1"
                                                         process="@this"
                                                         >
                                        </p:commandButton>
                                    </h:panelGrid>
                                    <h:panelGrid columns="3" cellpadding="2"  id="pInput1"  style="text-align: left">
                                        <h:outputLabel for="codProg" value="Código:" style="text-align: left"/>
                                        <h:outputText id="codProg" value="ABC000" style="text-align: left"/>
                                        <p:message for="codProg"></p:message>

                                        <p:outputLabel for="nombProg" value="Nombre del Programa:"/>
                                        <p:inputTextarea id="nombProg" value="#{programaVinculacionController.nombreProg}"  maxlength="150" rows="2" cols="80" required="#{not empty param['formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP1']}">
                                        </p:inputTextarea>
                                        <p:message for="nombProg"></p:message>

                                    </h:panelGrid>
                                    <h:panelGrid columns="2" id="panelNuevoCampus">
                                        <p:outputLabel for="dataTableCampus" value="Campus:" style="text-align: left"/>
                                        <p:commandButton id="btnNuevoCampus" value="Agregar" icon="ui-icon-plus" style="margin: 0; font-size: 11px; float: left"
                                                         title="Agregar Campus" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panelNuevoCampus, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panelIngresoCampus" action="#{programaVinculacionController.nuevoCampus()}" rendered="#{!programaVinculacionController.visualizarIngresoCampus}">
                                        </p:commandButton>
                                    </h:panelGrid>
                                    <p:panel id="panelIngresoCampus" visible="#{programaVinculacionController.visualizarIngresoCampus}" style="font-size: 11px; text-align: left; border: hidden; font-weight: bold">
                                        <h:panelGrid columns="2" cellpadding="5" cellspacing="0">
                                            <p:commandButton id="btnAgregarCampus" value="Aceptar" icon="ui-icon-locked" style="margin: 0; font-size: 11px; float: left " disabled="#{programaVinculacionController.habilitarAgregarCampus}"
                                                             title="Graba Información" action="#{programaVinculacionController.agregarCampus()}" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP1, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panelNuevoCampus,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panelIngresoCampus,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:dataTableCampus, :formPrincipal:formPrincipal1:message">
                                            </p:commandButton>
                                            <p:commandButton value="Cancelar" icon="ui-icon-arrowreturnthick-1-w" style="margin: 0; font-size: 11px; float: left "
                                                             title="Cancela Información" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panelNuevoCampus, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:panelIngresoCampus" action="#{programaVinculacionController.cancelarCampus}">
                                            </p:commandButton>
                                        </h:panelGrid>                         

                                        <p:selectOneMenu value="#{programaVinculacionController.codCampus}" style="width: 250px">
                                            <f:selectItem itemLabel="[[-Seleccione un item-]]" itemValue="" noSelectionOption="true"/>
                                            <f:selectItems value="#{programaVinculacionController.campusList}" var="cam" itemLabel="#{cam.stvcampDesc}" itemValue="#{cam.stvcampCode}"/>
                                            <p:ajax event="change" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnAgregarCampus" listener="#{programaVinculacionController.seleccionarCampus()}"/>
                                        </p:selectOneMenu>

                                    </p:panel>                                    

                                    <p:message for="dataTableCampus"></p:message>
                                    <p:dataTable id="dataTableCampus" value="#{programaVinculacionController.programaSelected.campusList}" var="campus"
                                                 style="width: 400px" paginator="false" rows="10" rowIndexVar="index"  resizableColumns="true" 
                                                 emptyMessage="No se encontraron resultados">
                                        <p:column headerText="Ord." width="20">
                                            <h:outputText value="#{index + 1}"/>
                                        </p:column>
                                        <p:column headerText="Código" width="40" style="white-space:pre-line" >
                                            <h:outputText value="#{campus.campus.stvcampCode}" style="white-space: pre-line; "/>
                                        </p:column>
                                        <p:column headerText="Nombre" style="white-space:pre-line" >
                                            <h:outputText value="#{campus.campus.stvcampDesc}" style="white-space: pre-line; "/>
                                        </p:column>      
                                        <p:column headerText="Eliminar" width="40" style="text-align: center">
                                            <p:commandButton style="margin:0" title="Eliminar el registro" icon="ui-icon-trash" 
                                                             action="#{programaVinculacionController.eliminarCampus()}"
                                                             update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:dataTableCampus, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP1">
                                                <f:setPropertyActionListener value="#{campus}" target="#{programaVinculacionController.svinDepcenproySelected}"/>
                                            </p:commandButton>

                                        </p:column>
                                    </p:dataTable>

                                    <p:spacer height="50" width="1"></p:spacer>


                                    <p:outputLabel value="PLAZO DE EJECUCIÓN" style="font-size: 14px"/>
                                    <h:panelGrid  columns="3" cellpadding="2" >
                                        <p:outputLabel for="fechInicio" value="Fecha de Inicio (dd/mm/aaaa):"/>
                                        <p:calendar id="fechInicio" navigator="true" value="#{programaVinculacionController.fechaInicio}" yearRange="2015:2030" size="10" style="font-size: 11px" locale="es" pattern="dd/MM/yyyy" required="#{not empty param['formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP1']}">
                                            <p:ajax event="dateSelect" listener="#{programaVinculacionController.calculaDuracion}" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:dur" />
                                        </p:calendar>
                                        <p:message for="fechInicio"></p:message>

                                        <p:outputLabel for="fechFin" value="Fecha de Finalización (dd/mm/aaaa):"/>
                                        <p:calendar id="fechFin" navigator="true" value="#{programaVinculacionController.fechaFin}" yearRange="2015:2030" size="10" style="font-size: 11px" locale="es" pattern="dd/MM/yyyy" required="#{not empty param['formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP1']}">
                                            <p:ajax event="dateSelect" listener="#{programaVinculacionController.calculaDuracion}" update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:dur" />
                                        </p:calendar>
                                        <p:message for="fechFin"></p:message>

                                        <p:outputLabel for="dur" value="Duración:"/>
                                        <p:outputLabel id="dur" value="#{programaVinculacionController.duracion}" style="font-weight: bold "/>                 

                                    </h:panelGrid>

                                    <p:spacer height="30"></p:spacer>

                                    <h:panelGrid columns="2">
                                        <p:outputLabel value="DATOS DEL DIRECTOR" style="font-size: 14px"/>
                                        <p:message for="lblNombreDirector"></p:message> 
                                    </h:panelGrid>
                                    <h:panelGrid columns="1">
                                        <p:outputLabel value="(Ingrese nombres y apellidos a buscar)"/>
                                        <p:autoComplete minQueryLength="3" value="#{programaVinculacionController.nomDocenteBusqueda}" completeMethod="#{programaVinculacionController.completeText}" size="70">
                                            <p:ajax event="itemSelect" listener="#{programaVinculacionController.onItemSelect}"  update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP1, :formPrincipal:formPrincipal1:tabViewPrincipal:tabP:datosResp"/>
                                        </p:autoComplete>
                                    </h:panelGrid>
                                    <h:panelGrid id="datosResp"  columns="3" cellpadding="2" > 
                                        <p:outputLabel value="Nombres:" style="text-align: left" />
                                        <p:outputLabel id="lblNombreDirector" value="#{programaVinculacionController.docente.usuNombres}" style="width:300px;font-weight: normal"/>
                                        <p:message for="lblCargoDirector"></p:message>

                                        <p:outputLabel value="Apellidos:" style="text-align: left"/>
                                        <p:outputLabel id="lblApellidosDirector" value="#{programaVinculacionController.docente.usuApellidos}" style="width:300px; font: xx-small; font-weight: normal"/>
                                        <p:message for="lblApellidosDirector"></p:message>

                                        <p:outputLabel value="Cargo:" style="text-align: left"/>
                                        <p:outputLabel id="lblCargoDirector" value="#{programaVinculacionController.docente.cargo}" style="width:300px; font: xx-small; font-weight: normal"/>
                                        <p:message for="lblCargoDirector"></p:message>

                                        <p:outputLabel value="Departamento:" style="text-align: left"/>
                                        <p:outputLabel id="lblDeptoDirector" value="#{programaVinculacionController.docente.departamento}" style="width:300px; font: xx-small; font-weight: normal"/>
                                        <p:message for="lblDeptoDirector"></p:message>

                                        <p:outputLabel for="txtMailDirector" value="Correo Electrónico:" style="text-align: left"/>
                                        <p:inputText id="txtMailDirector" value="#{programaVinculacionController.docente.usuEmail}" maxlength="50" required="false" style="width: 292px"
                                                     validatorMessage="Formato de correo incorrecto"> 
                                            <f:validateRegex pattern="(^[_A-Za-z0-9-\+]+(\.[_A-Za-z0-9-]+)*@[A-Za-z0-9-]+(\.[A-Za-z0-9]+)*(\.[A-Za-z]{2,})$)?" />
                                        </p:inputText>
                                        <p:message for="txtMailDirector" showDetail="true"></p:message>

                                        <p:outputLabel for="txtTelfDirector" value="Teléfono:" style="white-space: pre-line" /> 
                                        <p:inputText id="txtTelfDirector" value="#{programaVinculacionController.docente.usuTelefonoFijo}" maxlength="10" autocomplete="off" required="false" style="width: 80px">
                                        </p:inputText>
                                        <p:message for="txtTelfDirector"></p:message>
                                    </h:panelGrid>                                    


                                </p:panel>
                            </p:tab>
                            <!--tab2: Entidad Ejecutora-->
                            <p:tab title="Entidad Ejecutora" id="tabP2" disabled="#{programaVinculacionController.habilitaTab}">
                                <p:panel id="panel2" style="border: hidden">
                                    <h:panelGrid id="h2" columns="1" style="padding-bottom: 10px">
                                        <p:commandButton id="btnAgregarP2" value="Agregar" title="Agregar Información" icon="ui-icon-plus"
                                                         rendered="#{!programaVinculacionController.controlGrabarP2}"
                                                         update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:p2,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h2" 
                                                         action="#{programaVinculacionController.agregarP2()}" 
                                                         >
                                        </p:commandButton>

                                    </h:panelGrid>
                                    <p:panel id="p2" visible="#{programaVinculacionController.controlGrabarP2}" style="padding-left: 30px; border: hidden; ">
                                        <h:panelGrid columns="2" cellspacing="5">
                                            <p:commandButton id="btnGrabarP2" value="Grabar" title="Graba Información" icon="ui-icon-locked"  
                                                             action="#{programaVinculacionController.registraP2()}" 
                                                             update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:p2,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h2,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:dataTableProgVin, :formPrincipal:formPrincipal1:message"
                                                             onstart="PF('blockUIWidget').block()" oncomplete="PF('blockUIWidget').unblock()">
                                            </p:commandButton>
                                            <p:commandButton id="btnCancelarP2" value="Cancelar" title="Cancela Información" icon="ui-icon-arrowreturnthick-1-w" process="@this" 
                                                             update=":formPrincipal:formPrincipal1:tabViewPrincipal:tabP:p2,:formPrincipal:formPrincipal1:tabViewPrincipal:tabP:h2" 
                                                             action="#{programaVinculacionController.cancelarP2}">
                                            </p:commandButton>
                                        </h:panelGrid>                         
                                        <h:panelGrid columns="3" cellspacing="5" columnClasses="col1_entidadEjecutora">
                                            <p:outputLabel for="txtNomUnidadEjecutora" value="Nombre:"/>
                                            <p:inputText id="txtNomUnidadEjecutora" value="#{programaVinculacionController.entidadEjecutoraSelected.sopinNombre}" required="#{not empty param['formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP2']}" style="width: 450px" maxlength="100"></p:inputText>                            
                                            <p:message for="txtNomUnidadEjecutora"></p:message>

                                            <p:outputLabel for="txtDirUnidadEjecutora" value="Dirección:"/>
                                            <p:inputText id="txtDirUnidadEjecutora" value="#{programaVinculacionController.entidadEjecutoraSelected.sopinDireccion}" required="#{not empty param['formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP2']}" style="width: 450px" maxlength="100"></p:inputText>
                                            <p:message for="txtDirUnidadEjecutora"></p:message>
                                        </h:panelGrid>
                                        <h:panelGrid columns="3" cellspacing="5" columnClasses="col1_entidadEjecutora">
                                            <p:outputLabel for="selProvUnidadEjecutora" value="Provincia:"/>
                                            <p:selectOneMenu id="selProvUnidadEjecutora" value="#{programaVinculacionController.codProvinciaSelected}" style="width:250px;" required="#{not empty param['formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP2']}">
                                                <f:selectItem itemLabel="[[-Seleccione un item-]]" itemValue="0" noSelectionOption="true"/>
                                                <f:selectItems value="#{programaVinculacionController.provinciaList}" var="provin" itemLabel="#{provin.proNombre}" itemValue="#{provin.proId}"/>
                                            </p:selectOneMenu>
                                            <p:message for="selProvUnidadEjecutora"></p:message>
                                        </h:panelGrid>

                                        <h:panelGrid columns="6" cellspacing="5" columnClasses="col1_entidadEjecutora"> 
                                            <p:outputLabel for="txtCiudadUnidadEjecutora" value="Ciudad:"/>
                                            <p:inputText id="txtCiudadUnidadEjecutora" value="#{programaVinculacionController.entidadEjecutoraSelected.sopinCiudad}" required="#{not empty param['formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP2']}" maxlength="25"></p:inputText>
                                            <p:message for="txtCiudadUnidadEjecutora"></p:message>

                                            <p:outputLabel for="txtTelefUnidadEjecutora" value="Teléfono:" style="white-space: pre-line"/>
                                            <p:inputMask id="txtTelefUnidadEjecutora" value="#{programaVinculacionController.entidadEjecutoraSelected.sopinTelefono}" required="#{not empty param['formPrincipal:formPrincipal1:tabViewPrincipal:tabP:btnGrabarP2']}" mask="(999)99-9999999"></p:inputMask>
                                            <p:message for="txtTelefUnidadEjecutora"></p:message>
                                        </h:panelGrid>

                                        <h:panelGrid columns="3" cellspacing="5" columnClasses="col1_entidadEjecutora">
                                            <p:outputLabel for="txtPagWebUnidadEjecutora" value="Página Web:" style="text-align: left;"/>
                                            <p:inputText id="txtPagWebUnidadEjecutora" value="#{programaVinculacionController.entidadEjecutoraSelected.sopinPaguinaWeb}" style="width: 200px" maxlength="25"></p:inputText>
                                            <p:message for="txtPagWebUnidadEjecutora"></p:message>
                                        </h:panelGrid>
                                    </p:panel>
                                    <h:outputLabel value="Entidad ejecutora:"/>
                                    <p:dataTable id="dataTableProgVin" value="#{programaVinculacionController.entidadEjecutoraList}" var="progVin"
                                                 style="width: 100%; padding-top: 5px" paginator="true" rows="10" rowIndexVar="index"  resizableColumns="true" 
                                                 emptyMessage="No se encontraron resultados">
                                        <p:column headerText="Ord." style="width:20px">
                                            <h:outputText value="#{index + 1}"/>
                                        </p:column>
                                        <p:column headerText="Nombre" style="white-space:pre-line" >
                                            <h:outputText value="#{progVin.sopinNombre}" style="white-space: pre-line; "/>
                                        </p:column>
                                        <p:column headerText="Teléfono" style="width: 80px;white-space:pre-line" >
                                            <h:outputText value="#{progVin.sopinTelefono}" style="white-space: pre-line; "/>
                                        </p:column>     
                                        <p:column headerText="Dirección" style="width: 100px;white-space:pre-line" >                               
                                            <h:outputText value="#{progVin.sopinDireccion}" style="white-space: pre-line; width: 100%"/>
                                        </p:column>
                                        <p:column headerText="Ciudad" style="width: 100px;white-space:pre-line" >
                                            <h:outputText value="#{progVin.sopinCiudad}" style="white-space: pre-line; width: 100%"/>
                                        </p:column> 
                                        <p:column headerText="Provincia" style="width:100px">
                                            <h:outputText value="#{progVin.proId.proNombre}" style="white-space: pre-line; width: 100%"/>
                                        </p:column>
                                        <p:column headerText="Página Web" style="width: 100px;white-space:pre-line" >
                                            <h:outputText value="#{progVin.sopinPaguinaWeb}" style="white-space: pre-line; width: 100%"/>
                                        </p:column>                           
